{
  "name": "schemax-vscode",
  "displayName": "SchemaX",
  "description": "Design and manage Databricks Unity Catalog schemas visually. Version control, SQL generation, apply/rollback/import via CLI. Install Python SDK (pip install schemaxpy) for full features.",
  "publisher": "schematic-dev",
  "version": "0.2.1",
  "author": {
    "name": "Field Engineering"
  },
  "contributors": [
    {
      "name": "SchemaX Contributors",
      "url": "https://github.com/vb-dbrks/schemax/graphs/contributors"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vb-dbrks/schemax-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/vb-dbrks/schemax-vscode/issues"
  },
  "homepage": "https://github.com/vb-dbrks/schemax-vscode#readme",
  "keywords": [
    "databricks",
    "unity catalog",
    "schema",
    "database",
    "migration",
    "version control",
    "delta lake",
    "data governance",
    "visual designer",
    "catalog"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Other"
  ],
  "icon": "images/icon128.png",
  "galleryBanner": {
    "color": "#1a1a2e",
    "theme": "dark"
  },
  "qna": "marketplace",
  "activationEvents": [
    "onCommand:schemax.openDesigner",
    "onCommand:schemax.showLastOps",
    "onCommand:schemax.createSnapshot",
    "onCommand:schemax.generateSQL",
    "onCommand:schemax.importAssets",
    "onCommand:schemax.installPythonSdk",
    "onView:schemax.welcome"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "schemax.openDesigner",
        "title": "SchemaX: Open Designer"
      },
      {
        "command": "schemax.showLastOps",
        "title": "SchemaX: Show Last Emitted Changes"
      },
      {
        "command": "schemax.createSnapshot",
        "title": "SchemaX: Create Snapshot"
      },
      {
        "command": "schemax.generateSQL",
        "title": "SchemaX: Generate SQL Migration"
      },
      {
        "command": "schemax.importAssets",
        "title": "SchemaX: Import Existing Assets"
      },
      {
        "command": "schemax.installPythonSdk",
        "title": "SchemaX: Install Python SDK (pip install schemaxpy)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "schemax",
          "title": "SchemaX",
          "icon": "images/activity-bar.svg"
        }
      ]
    },
    "views": {
      "schemax": [
        {
          "id": "schemax.welcome",
          "name": "SchemaX"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "schemax.welcome",
        "contents": "Design and manage Unity Catalog schemas visually.\n[Open SchemaX Designer](command:schemax.openDesigner)\n[Generate SQL Migration](command:schemax.generateSQL)\n[Create Snapshot](command:schemax.createSnapshot)"
      }
    ]
  },
  "scripts": {
    "build": "npm run build:webview && npm run build:ext",
    "build:webview": "vite build",
    "build:ext": "node esbuild.config.mjs",
    "watch": "concurrently \"npm:watch:webview\" \"npm:watch:ext\"",
    "watch:webview": "vite build --watch",
    "watch:ext": "node esbuild.config.mjs --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node scripts/prompt-e2e-env.js",
    "package": "vsce package --no-dependencies",
    "deploy": "vsce publish --no-dependencies",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.90.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vscode/vsce": "^3.6.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.27.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.4.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.0",
    "@rollup/rollup-linux-x64-gnu": "^4.52.0"
  }
}
