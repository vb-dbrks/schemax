{
  "name": "schematic-vscode",
  "displayName": "Schematic",
  "description": "Visual designer for Databricks Unity Catalog schemas with version control",
  "publisher": "varunbhandary",
  "version": "0.1.0",
  "author": {
    "name": "Field Engineering"
  },
  "contributors": [
    {
      "name": "Varun Bhandary",
      "url": "https://github.com/vb-dbrks"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vb-dbrks/Schematic.git"
  },
  "bugs": {
    "url": "https://github.com/vb-dbrks/Schematic/issues"
  },
  "homepage": "https://github.com/vb-dbrks/Schematic#readme",
  "keywords": [
    "databricks",
    "unity catalog",
    "schema",
    "database",
    "migration",
    "version control",
    "visual designer"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "schematic.openDesigner",
        "title": "Schematic: Open Designer"
      },
      {
        "command": "schematic.showLastOps",
        "title": "Schematic: Show Last Emitted Changes"
      },
      {
        "command": "schematic.createSnapshot",
        "title": "Schematic: Create Snapshot"
      },
      {
        "command": "schematic.generateSQL",
        "title": "Schematic: Generate SQL Migration"
      }
    ]
  },
  "scripts": {
    "build": "npm run build:webview && npm run build:ext",
    "build:webview": "vite build",
    "build:ext": "node esbuild.config.mjs",
    "watch": "concurrently \"npm:watch:webview\" \"npm:watch:ext\"",
    "watch:webview": "vite build --watch",
    "watch:ext": "node esbuild.config.mjs --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "package": "vsce package --no-dependencies",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.90.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vscode/vsce": "^3.6.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.4.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.0",
    "@rollup/rollup-linux-x64-gnu": "^4.52.0"
  }
}
